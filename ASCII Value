#include <stdio.h>
int main() {  
    char c;
    printf("Enter a character: ");
    scanf("%c", &c);  
    
    // %d displays the integer value of a character// is the most common character encoding format for text data in computers and on the internet.
    // %c displays the actual character//
    printf("ASCII value of %c = %d", c, c);
    
    return 0;
}
